package testPjt2;

import testPjt2.vo.Battery;

public class ElectronicRoboyToy {
	private Battery battery;
	
	public ElectronicRoboyToy() {
		
	}
	
	void setBattery(Battery battery) {
		this.battery = battery;
	}
	
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return "RoboyToy의 배터리는 " + battery.battery + "입니다.";
	}
	
	
}
